# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/fmengo?schema=public"

# JWT Authentication
JWT_SECRET="your-jwt-secret-key"
JWT_EXPIRATION="1d"

# Google OAuth
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# Facebook OAuth
FACEBOOK_APP_ID="your-facebook-app-id"
FACEBOOK_APP_SECRET="your-facebook-app-secret"

# Telegram Bot
TELEGRAM_BOT_TOKEN="your-telegram-bot-token"

# Server Configuration
PORT=3000
NODE_ENV="development"

# Crypto Payment API Keys
CRYPTO_PAYMENT_API_KEY="your-crypto-payment-api-key"
CRYPTO_PAYMENT_API_SECRET="your-crypto-payment-api-secret"

# Storage Configuration
STORAGE_BUCKET="fmengo-storage"
STORAGE_REGION="us-east-1"
STORAGE_ACCESS_KEY="your-storage-access-key"
STORAGE_SECRET_KEY="your-storage-secret-key"

# Email Service
EMAIL_HOST="smtp.example.com"
EMAIL_PORT=587
EMAIL_USER="your-email-user"
EMAIL_PASSWORD="your-email-password"
EMAIL_FROM="noreply@fmengo.com"
